<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/bottomtoolbarmain"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/main_content">
    <!-- HEADER NAVIGATION -->
        <LinearLayout
            android:orientation="horizontal"
            android:layout_alignParentTop="true"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/white"
            android:id="@+id/header"
            android:showDividers="end">
            <LinearLayout
                android:id="@+id/header_title"
                android:layout_weight="3"
                android:layout_width="0dp"
                android:layout_marginLeft="17dp"
                android:layout_height="match_parent">
                <TextView
                    android:id="@+id/heading_text"
                    android:fontFamily="sans-serif"
                    android:textSize="22dp"
                    android:textColor="#4eb4e3"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:gravity="center"
                    android:text="James Salib Aba" />
            </LinearLayout>
          <LinearLayout
              android:id="@+id/header_menu_right"
              android:layout_weight="1"
              android:layout_width="0dp"
              android:layout_marginTop="5dp"
              android:layout_height="match_parent">
            <ImageView
                android:src="@drawable/emg"
                android:layout_width="58.5dp"
                android:layout_height="20dp"
                android:id="@+id/imageView2" />
          </LinearLayout>
        </LinearLayout>
    <!-- CONTENT -->
        <LinearLayout
            android:layout_below="@+id/header"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:id="@+id/content"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_marginTop="0.0dp">
            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="25dp"
                android:id="@+id/tableLayout1">
                <TableRow
                    android:id="@+id/tableRow2">
                    <ImageButton
                        android:src="@drawable/ic_icon1"
                        android:layout_column="0"
                        android:id="@+id/imageButton1"
                        android:layout_width="200dp"
                        android:layout_height="154.5dp" />
                    <ImageButton
                        android:src="@drawable/ic_icon1"
                        android:layout_column="1"
                        android:id="@+id/imageButton2"
                        android:layout_width="200dp"
                        android:layout_height="154.5dp" />
                </TableRow>
                <TableRow
                    android:id="@+id/tableRow3">
                    <TextView
                        android:text="Medications"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:layout_column="0"
                        android:id="@+id/textView14"
                        android:textColor="#4eb4e3"
                        android:fontFamily="sans-serif"
                        android:gravity="center" />
                    <TextView
                        android:text="Visits"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:id="@+id/textView20"
                        android:textColor="#4eb4e3"
                        android:fontFamily="sans-serif"
                        android:gravity="center" />
                </TableRow>
            </TableLayout>
        </LinearLayout>
    <!-- FOOTER NAVIGATION #4e8ae3-->
        <LinearLayout
            android:id="@+id/footer_border"
            android:background="#414655"
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_above="@+id/footer" />
        <LinearLayout
            android:orientation="horizontal"
            android:background="#414655"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_alignParentBottom="true"
            android:id="@+id/footer">
            <LinearLayout
                android:orientation="vertical"
                android:layout_height="match_parent"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_marginLeft="8dp"
                android:id="@+id/tab1">
                <ImageButton
                    android:id="@+id/tab1_icon"
                    android:layout_weight="2"
                    android:layout_height="0dp"
                    android:layout_width="match_parent"
                    android:background="#414655"
                    android:src="@drawable/homesmall"
                    android:clickable="true"
                    android:fontFamily="sans-serif-light"
                    android:layout_marginBottom="0.0dp" />
                <TextView
                    android:gravity="center"
                    android:id="@+id/tab1_text"
                    android:layout_weight="1"
                    android:textColor="@color/white"
                    android:textSize="12dp"
                    android:layout_height="0dp"
                    android:layout_width="match_parent"
                    android:layout_marginTop="0.0dp"
                    android:layout_marginBottom="5dp"
                    android:text="Home page" />
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:layout_height="match_parent"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:id="@+id/tab2">
                <ImageButton
                    android:id="@+id/tab2_icon"
                    android:layout_weight="2"
                    android:layout_height="0dp"
                    android:layout_width="match_parent"
                    android:background="#414655"
                    android:src="@drawable/calendarsmall"
                    android:clickable="true"
                    android:layout_marginBottom="0.0dp"
                    android:fontFamily="sans-serif-light" />
                <TextView
                    android:gravity="center"
                    android:layout_weight="1"
                    android:id="@+id/tab2_text"
                    android:textColor="@color/white"
                    android:textSize="12dp"
                    android:layout_height="0dp"
                    android:layout_width="match_parent"
                    android:layout_marginTop="0.0dp"
                    android:layout_marginBottom="5dp"
                    android:text="Calendar" />
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:layout_height="match_parent"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:id="@+id/tab3">
                <ImageButton
                    android:id="@+id/tab3_icon"
                    android:layout_weight="2"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:background="#414655"
                    android:src="@drawable/chatsmall"
                    android:clickable="true"
                    android:layout_marginBottom="0.0dp"
                    android:fontFamily="sans-serif-light" />
                <TextView
                    android:gravity="center"
                    android:id="@+id/tab3_text"
                    android:layout_weight="1"
                    android:textColor="@color/white"
                    android:textSize="12dp"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginTop="0.0dp"
                    android:layout_marginBottom="5dp"
                    android:text="Chat" />
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:layout_height="match_parent"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:id="@+id/tab4">
                <ImageButton
                    android:id="@+id/tab4_icon"
                    android:layout_weight="2"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:background="#414655"
                    android:layout_marginBottom="0.0dp"
                    android:src="@drawable/settingsmall2"
                    android:clickable="true"
                    android:fontFamily="sans-serif-light" />
                <TextView
                    android:gravity="center"
                    android:id="@+id/tab4_text"
                    android:layout_weight="1"
                    android:textColor="@color/white"
                    android:textSize="12dp"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginTop="0.0dp"
                    android:layout_marginBottom="5dp"
                    android:text="Settings" />
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:layout_height="match_parent"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:id="@+id/tab4">
                <ImageButton
                    android:id="@+id/tab5_icon"
                    android:layout_weight="2"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:background="#414655"
                    android:src="@drawable/notifiactionsmall"
                    android:clickable="true"
                    android:layout_marginBottom="0.0dp"
                    android:fontFamily="sans-serif-light" />
                <TextView
                    android:gravity="center"
                    android:id="@+id/tab5_text"
                    android:layout_weight="1"
                    android:textColor="@color/white"
                    android:textSize="12dp"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginTop="0.0dp"
                    android:layout_marginBottom="5dp"
                    android:text="Notification" />
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>
</RelativeLayout>